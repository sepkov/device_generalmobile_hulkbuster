/boot		   emmc		/dev/bootimg
/logo          emmc     /dev/logo flags=display="Logo";backup=1
/system		   ext4		/dev/block/mmcblk0p5 flags=display="System";backup=1;canbewiped;wipeingui
/cache		   ext4		/emmc@cache
/data		   ext4		/emmc@usrdata
/nvram 		   emmc     /dev/nvram flags=display="SN/NVRAM";backup=1
/sec_ro 	   emmc     /dev/sec_ro flags=subpartitionof=/nvram;backup=1
/recovery	   emmc		/dev/recovery flags=display="Recovery";backup=1;
/pro_info	   emmc     /dev/pro_info flags=subpartitionof=/nvram;backup=1
/usbotg		   auto     /dev/block/sda1			/dev/block/sda			flags=display="Usb-Otg";storage;wipeingui;removable;backup=0